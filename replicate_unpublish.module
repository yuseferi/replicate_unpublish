<?php

/**
 * @file
 * Contains replicate_unpublish.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function replicate_unpublish_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the replicate_unpublish module.
    case 'help.page.replicate_unpublish':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Unpublished node after Replication by Replicate Module') . '</p>';
      return $output;

    default:
  }
}
